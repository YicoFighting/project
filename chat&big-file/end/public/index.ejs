<!DOCTYPE html>
<html lang="zh">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>文件中装站</title>
    <style>
      a {
        display: block;
        color: black;
        text-decoration: none;
      }
      a:hover {
        color: #1890ff;
      }
    </style>
  </head>
  <body>
    <% data.forEach(function(file){%>
    <a href="http://localhost:<%- port %>/image/<%- file.split('.')[0] %>">
      <%- file.split('.')[0] %>
    </a>
    <% })%>
  </body>
</html>
